{
  "name": "heap-analyzer",
  "version": "1.0.3",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "heap-analyzer": "dist/cli.js"
  },
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "snapshots",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/index.ts",
    "dev:watch": "tsc --watch",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "clean": "rm -rf snapshots && mkdir snapshots",
    "release-patch": "npm test && npm run build && npm version patch && npm publish",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "heap",
    "memory",
    "analysis",
    "profiling",
    "debugging",
    "performance"
  ],
  "author": "Conor-Hinchee",
  "license": "ISC",
  "description": "A powerful heap analyzer for Node.js memory profiling and leak detection",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Conor-Hinchee/heap-analyzer.git"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^24.0.7",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "esbuild": "^0.19.0",
    "jest": "^30.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-jest": "^29.4.5",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@memlab/core": "^1.1.43",
    "@memlab/lens": "^1.0.3"
  }
}
